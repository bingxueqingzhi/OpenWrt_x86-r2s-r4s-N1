
CONFIG_TARGET_x86=y
CONFIG_TARGET_x86_64=y
CONFIG_TARGET_ROOTFS_TARGZ=y

# EFI支持:
CONFIG_GRUB_IMAGES=y
CONFIG_GRUB_EFI_IMAGES=n
CONFIG_VMDK_IMAGES=y

# 其他需要安装的软件包:
CONFIG_PACKAGE_luci-ssl=n # uhttpd服务
CONFIG_PACKAGE_luci-ssl-nginx=y # nginx
#CONFIG_PACKAGE_kmod-r8168=n

CONFIG_OPENSSL_PREFER_CHACHA_OVER_GCM=n



# QZ mod
CONFIG_DEFAULT_kmod-usb2=y
CONFIG_DEFAULT_kmod-usb3=n
CONFIG_PACKAGE_kmod-e100=y
CONFIG_DEFAULT_kmod-e1000=y
CONFIG_DEFAULT_kmod-e1000e=y
CONFIG_PACKAGE_kmod-igb=y
CONFIG_PACKAGE_kmod-igc=y
CONFIG_DEFAULT_kmod-ixgbe=y
CONFIG_PACKAGE_kmod-ixgbevf=y
CONFIG_PACKAGE_kmod-i40e=y
